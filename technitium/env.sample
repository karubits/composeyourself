# Technitium DNS Server â€“ copy to .env and set values
# These are read only on first run when the DNS config file does not exist.
# https://github.com/TechnitiumSoftware/DnsServer

# Primary domain name for this DNS server
DNS_SERVER_DOMAIN=dns-server

# Web console admin password (or use DNS_SERVER_ADMIN_PASSWORD_FILE)
# DNS_SERVER_ADMIN_PASSWORD=your-secure-password
# DNS_SERVER_ADMIN_PASSWORD_FILE=/path/to/password.txt

# IPv6 preference for queries
# DNS_SERVER_PREFER_IPV6=false

# Web console bind address (comma-separated; use with host network mode)
# DNS_SERVER_WEB_SERVICE_LOCAL_ADDRESSES=172.17.0.1,127.0.0.1
# DNS_SERVER_WEB_SERVICE_HTTP_PORT=5380
# DNS_SERVER_WEB_SERVICE_HTTPS_PORT=53443
# DNS_SERVER_WEB_SERVICE_ENABLE_HTTPS=false
# DNS_SERVER_WEB_SERVICE_USE_SELF_SIGNED_CERT=false
# DNS_SERVER_WEB_SERVICE_TLS_CERTIFICATE_PATH=/etc/dns/tls/cert.pfx
# DNS_SERVER_WEB_SERVICE_TLS_CERTIFICATE_PASSWORD=
# DNS_SERVER_WEB_SERVICE_HTTP_TO_TLS_REDIRECT=false

# DNS-over-HTTP (e.g. behind TLS reverse proxy)
# DNS_SERVER_OPTIONAL_PROTOCOL_DNS_OVER_HTTP=false

# Recursion: Allow | Deny | AllowOnlyForPrivateNetworks | UseSpecifiedNetworkACL
# DNS_SERVER_RECURSION=AllowOnlyForPrivateNetworks
# DNS_SERVER_RECURSION_NETWORK_ACL=192.168.10.0/24,!192.168.10.2
# Obsolete (use RECURSION_NETWORK_ACL with ! prefix to deny):
# DNS_SERVER_RECURSION_DENIED_NETWORKS=1.1.1.0/24
# DNS_SERVER_RECURSION_ALLOWED_NETWORKS=127.0.0.1,192.168.1.0/24

# Blocking (Blocked Zone / Block List Zone)
# DNS_SERVER_ENABLE_BLOCKING=false
# DNS_SERVER_ALLOW_TXT_BLOCKING_REPORT=false
# DNS_SERVER_BLOCK_LIST_URLS=

# Forwarders (comma-separated) and protocol: Udp | Tcp | Tls | Https | HttpsJson
# DNS_SERVER_FORWARDERS=1.1.1.1,8.8.8.8
# DNS_SERVER_FORWARDER_PROTOCOL=Tcp

# Logging
# DNS_SERVER_LOG_USING_LOCAL_TIME=true
